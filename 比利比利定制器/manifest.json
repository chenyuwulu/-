{
	"manifest_version": 2,
	"name": "比利比利清道妇",
	"version": "1.0",
	"description": "demo版",
	"author": "FAQ",
	"icons":
	{
		"48": "icon.png",
		"128": "icon.png"
	},
	"browser_action": 
	{
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"permissions":
	[
		"contextMenus",
		"tabs",
		"notifications",
		"webRequest",
		"webRequestBlocking",
		"storage",
		"declarativeContent",
		"http://*/*",
		"https://*/*"
	],
	"content_scripts": 
	[
		{
			"matches": ["<all_urls>"],
			"js": ["js/jquery-1.8.3.js","js/content-start.js"],
			"run_at": "document_start"
		},
		{
			"matches":["https://www.bilibili.com/*"],
			"js":["js/content-end.js"],
			"run_at":"document_end"
		}
	],
	"web_accessible_resources": 
	[
		"inject.js"
	],
	"background":
	{
		"page": "background.html"
	}
}